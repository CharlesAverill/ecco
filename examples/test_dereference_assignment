int main() {
    int a; int b; int c;
    a = b = c = 3;
    print a; print b; print c; // 3 3 3

    int x; int *y;
    y = &x; *y = 5;
    print x;                   // 5
    print &x == y;             // 1
    print *y == x;             // 1

    int n;
    n = 50;
    int **z; z = &y; **z = n;
    print x;                  // 50
    print *y;                 // 50

    // We will eventually un-comment this field
    // when we add local variables
    int **q; q = &y; 
    print **q;                // 50
    *q = &a;
    *y = 7;
    print a;                  // 7
}
