int main() {
    int x;
    x = 0 + 3; print x; // 3
    x = 3 + 0; print x; // 3
    x = 3 - 0; print x; // 3
    x = 0 - 3; print x; // -3 
    x = 0 * 3; print x; // 0
    x = 3 * 0; print x; // 0
    x = 0 / 3; print x; // 0
    x = 1 + 2 + 3 + 4; print x; // 10
    x = 5 * 4 * 3 * 2 * 1; print x; // 120
    x = 100 / 10 / 2; print x; // 5
    x = 10 + x + 2; print x; // (10 + x) + 2 = (10 + 2) + x = 17
    x = 10 * x * 2; print x; // (10 * x) * 2 = (10 * 2) * x = 340
    x = 350 - x - 2; print x; // (350 - x) - 2 = 350 - (2 + x) = 8
    x = 800 / x / 2; print x; // (800 / x) / 2 = 800 / (x * 2) = 50
}
