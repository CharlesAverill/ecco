int main() {
    int a; int b;
    int c; int d;
    int *e; int *f;

    a = 18; print a;
    e = &a; b = *e; print b;

    c = 12; print c;
    f = &c; d = *f; print d;

    int g; int *h; int **j; int ***k;
    g = 5; h = &g; j = &h; k = &j;
    int n; int *m; int **l;
    l = *k; m = *l; n = *m;
    print n;
}
